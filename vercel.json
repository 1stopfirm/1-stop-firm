{
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "",
  "devCommand": "",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/mobile", "destination": "/mobile/index.html" },
    { "source": "/mobile/", "destination": "/mobile/index.html" },
    { "source": "/mobile/about", "destination": "/mobile/about.html" },
    { "source": "/mobile/services", "destination": "/mobile/services.html" },
    { "source": "/mobile/digital-marketing", "destination": "/mobile/digital-marketing.html" },
    { "source": "/mobile/website-development", "destination": "/mobile/website-development.html" },
    { "source": "/mobile/brand-development", "destination": "/mobile/brand-development.html" },
    { "source": "/mobile/strategy-consultation", "destination": "/mobile/strategy-consultation.html" },
    { "source": "/mobile/blogs", "destination": "/mobile/blogs.html" },
    { "source": "/mobile/contact", "destination": "/mobile/contact.html" },
    { "source": "/mobile/thank-you", "destination": "/mobile/thank-you.html" },
    { "source": "/mobile/(.*)", "destination": "/mobile/$1" },
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact-us.html"
    },
    {
      "source": "/blogs",
      "destination": "/blogs.html"
    },
    {
      "source": "/insights",
      "destination": "/blogs.html"
    },
    {
      "source": "/brand-development",
      "destination": "/brand-development.html"
    },
    {
      "source": "/digital-marketing",
      "destination": "/digital-marketing-service.html"
    },
    {
      "source": "/web-development",
      "destination": "/fresh-website-development.html"
    },
    {
      "source": "/website-development",
      "destination": "/fresh-website-development.html"
    },
    {
      "source": "/strategy-consultation",
      "destination": "/strategy-consultation.html"
    },
    {
      "source": "/thank-you",
      "destination": "/thank-you.html"
    }
  ],
  "redirects": [
    { "source": "/mobile/index.html", "destination": "/mobile/", "permanent": true },
    { "source": "/mobile/index", "destination": "/mobile/", "permanent": true }
  ]
} 